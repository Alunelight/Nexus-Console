# Database
DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost:5432/nexus_console

# Redis
REDIS_URL=redis://localhost:6379/0

# Application
APP_NAME=Nexus Console API
APP_VERSION=0.1.0
DEBUG=True
# 生成安全的 SECRET_KEY: python -c 'import secrets; print(secrets.token_urlsafe(32))'
SECRET_KEY=your-secret-key-here-min-32-chars-change-in-production

# Logging
LOG_LEVEL=INFO

# CORS
CORS_ORIGINS=["http://localhost:5173","http://localhost:3000"]

# Celery
CELERY_BROKER_URL=redis://localhost:6379/1
CELERY_RESULT_BACKEND=redis://localhost:6379/2
